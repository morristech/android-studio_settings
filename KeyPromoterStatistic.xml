<application>
  <component name="KeyPromoterStatistic">
    <option name="statistics">
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Left' or 'Button4 Click'</ShortCut>
          <Description>Back</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit Changes...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit Changes...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit File...">
        <Item>
          <IdeaActionID>CheckinFiles</IdeaActionID>
          <ShortCut>'Alt+I'</ShortCut>
          <Description>Commit File...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Compare">
        <Item>
          <ShortCut>'Button1 Double-Click'</ShortCut>
          <Description>Compare</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Directory">
        <Item>
          <IdeaActionID>NewDir</IdeaActionID>
          <ShortCut>'Ctrl+N, P'</ShortCut>
          <Description>Directory</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide">
        <Item>
          <ShortCut>'Shift+Escape'</ShortCut>
          <Description>Hide</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Messages">
        <Item>
          <IdeaActionID>ActivateMessagesToolWindow</IdeaActionID>
          <ShortCut>'Alt+0'</ShortCut>
          <Description>Messages</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Package">
        <Item>
          <IdeaActionID>NewDir</IdeaActionID>
          <ShortCut>'Ctrl+N, P'</ShortCut>
          <Description>Package</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Remove">
        <Item>
          <ShortCut>'Alt+Delete'</ShortCut>
          <Description>Remove</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore Popup">
        <Item>
          <ShortCut>'Ctrl+Q' or 'Alt+Button2 Click'</ShortCut>
          <Description>Restore Popup</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show History">
        <Item>
          <IdeaActionID>Vcs.ShowTabbedFileHistory</IdeaActionID>
          <ShortCut>'Alt+H'</ShortCut>
          <Description>Show History</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Android Monitor">
        <Item>
          <IdeaActionID>ActivateAndroidMonitorToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>Android Monitor</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Exclude">
        <Item>
          <IdeaActionID>UsageView.Exclude</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Exclude</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rebuild Project">
        <Item>
          <IdeaActionID>RebuildGradleProject</IdeaActionID>
          <Description>Rebuild Project</Description>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'Alt+4'</ShortCut>
          <Description>Run</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'app'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'app'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings Repository…">
        <Item>
          <IdeaActionID>ConfigureIcs</IdeaActionID>
          <Description>Settings Repository…</Description>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <Description>Terminal</Description>
        </Item>
      </Statistic>
    </option>
  </component>
</application>