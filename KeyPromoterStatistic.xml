<application>
  <component name="KeyPromoterStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Messages">
        <Item>
          <IdeaActionID>ActivateMessagesToolWindow</IdeaActionID>
          <ShortCut>'Alt+0'</ShortCut>
          <Description>Messages</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Android Monitor">
        <Item>
          <IdeaActionID>ActivateAndroidMonitorToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>Android Monitor</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'Alt+4'</ShortCut>
          <Description>Run</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'app'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'app'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <Description>Terminal</Description>
        </Item>
      </Statistic>
    </option>
  </component>
</application>