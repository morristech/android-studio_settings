<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug 'app'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'app'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>22</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Into">
        <Item>
          <IdeaActionID>StepInto</IdeaActionID>
          <ShortCut>'F7'</ShortCut>
          <Description>Step Into</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Out">
        <Item>
          <IdeaActionID>StepOut</IdeaActionID>
          <ShortCut>'Shift+F8'</ShortCut>
          <Description>Step Out</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Over">
        <Item>
          <IdeaActionID>StepOver</IdeaActionID>
          <ShortCut>'F8'</ShortCut>
          <Description>Step Over</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'app'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'app'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Android Monitor">
        <Item>
          <IdeaActionID>ActivateAndroidMonitorToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>Android Monitor</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Refresh all Gradle projects">
        <Item>
          <IdeaActionID>ExternalSystem.RefreshAllProjects</IdeaActionID>
          <ShortCut>'Ctrl+G, S'</ShortCut>
          <Description>Refresh all Gradle projects</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>